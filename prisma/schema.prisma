datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            Int    @id @default(autoincrement())
  address       String @unique
  message       String
  tncDocVersion String @default("1.0")

  createdAt DateTime @default(now())
}
